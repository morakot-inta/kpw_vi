{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST"],
      "cors": {
        "allowedOrigins": ["*"]
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}

